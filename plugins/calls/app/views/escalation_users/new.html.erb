<%= title [l(:label_call_settings), plugin_settings_path(id: 'calls')], l(:label_escalation_user_new) %>

<%= labelled_form_for @escalation_user, :html => {:multipart => true} do |f| %>
  <%= render :partial => 'form', :locals => { :f => f} %>
  <p>
    <%= submit_tag l(:button_create) %>
    <%= submit_tag l(:button_create_and_continue), :name => 'continue' %>
  </p>
<% end %>

